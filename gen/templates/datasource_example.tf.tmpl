{{- if .DocumentationExamples}}
{{ range $key := .DocumentationExamples}}data "aci_{{$.ResourceName}}" "example" {
  {{- if $.HasParent}}
  parent_dn = {{createParentDnValue $key "example" $.Definitions}}
  {{- end}}
  {{- range $.Properties}}{{- if .IsNaming}}
  {{overwriteProperty .PkgName .SnakeCaseName $.Definitions}} = "{{lookupTestValue .PkgName .SnakeCaseName $.TestVars $.Definitions}}"{{- end}}
  {{- end}}
}

{{ end}}
{{- else}}data "aci_{{$.ResourceName}}" "example" {
  {{- range $.Properties}}{{- if .IsNaming}}
  {{overwriteProperty .PkgName .SnakeCaseName $.Definitions}} = "{{lookupTestValue .PkgName .SnakeCaseName $.TestVars $.Definitions}}"{{- end}}
  {{- end}}
}
{{ end}}