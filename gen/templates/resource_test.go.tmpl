                    {{- if eq $child_key "children"}}
                        {{- $newCtx := addToChild $child_value "ResourceName" $resourceName "InheritedKey" (print $inheritedKey "." $key "." 0) -}}
                        {{- template "testNestedChildrenWithNoParentAndFirstChildRemoved" $newCtx }}
