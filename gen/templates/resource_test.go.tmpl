					resource.TestCheckResourceAttr("{{$resourceName}}", "{{$inheritedKey}}.{{$key}}.0.{{$child_key}}", "{{$child_value}}"),
							{{- end}}
						{{- end}}
					{{- end}}
					resource.TestCheckResourceAttr("{{$resourceName}}", "{{$inheritedKey}}.{{$key}}.#", "1"),
			{{- end}}
		{{- end}}
    {{- end}}

