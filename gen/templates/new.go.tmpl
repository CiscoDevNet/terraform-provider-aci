package convert_funcs


func dn_to_class(prefix string) string {

    mapping := map[string]string{
    
        {{- range $pkgName, $model := .Models }}
    
        {{- range $childClass := $model.ChildClasses }}

        "{{ $childClass }}-{{ extractPrefix $model.RnFormat }}": 
           "{{ lowercaseFirst $model.ResourceClassName }}",
      
        {{- end }}
        {{- end }}
    }

   if className, found := mapping[prefix]; found {
        return className
    }

    return ""
} 